// Example program #include #include #include

using namespace std;

void imprime_numeros(int v[], int tamanho) { if (tamanho < 0) { cout << "erro tamanho foi negativo! " << endl; return; }

for(int i = 0; i < tamanho; i++) {
    cout << v[i] << endl;
}
}

int main () { int array[3] = {24, 6, 94};

imprime_numeros(array,-3);
}
